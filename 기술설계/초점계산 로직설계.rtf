  \cf3 let\cf2  referenceRatio = referenceFaceWidth / referenceShoulderWidth\
  \cf3 let\cf2  currentRatio = currentFaceWidth / currentShoulderWidth\
\
  \cf5 // \uc0\u48708 \u50984 \u51060  \u44057 \u51004 \u47732  \u44057 \u51008  \u54868 \u44033 \cf2 \
  \cf3 let\cf2  ratioMatch = \cf10 abs\cf2 (referenceRatio - currentRatio) < threshold\
