\f0\b \uc0\u47928 \u51228 
\f1\b0 : iOS\uc0\u45716  \u51221 \u54869 \u54620  \u49468 \u49436  \u53356 \u44592 \u47484  API\u47196  \u51228 \u44277 \u54616 \u51648  \u50506 \u51020 \
\
  
\f0\b \uc0\u54644 \u44208 \u52293 
\f1\b0 :\
  \cf5 // \uc0\u46356 \u48148 \u51060 \u49828  \u47784 \u45944 \u48324  \u54616 \u46300 \u53076 \u46377 \u46108  \u49468 \u49436  \u51221 \u48372 \cf2 \
  \cf3 struct DeviceSensorDatabase \cf2 \{\
      \cf3 static\cf2  \cf3 func\cf9  getSensorInfo()\cf2  -> \cf4 SensorInfo\cf2 ? \{\
          \cf3 var\cf2  systemInfo = utsname()\
          uname(&systemInfo)\
          \cf3 let\cf2  modelCode = \cf4 String\cf2 (cString: &systemInfo.machine.\cf5 0\cf2 )\
\
          \cf5 // iPhone \uc0\u47784 \u45944 \u48324  \u49468 \u49436  \u51221 \u48372  \u47588 \u54609 \cf2 \
          \cf3 switch\cf2  modelCode \{\
          \cf3 case\cf2  \cf6 "iPhone16,1"\cf2 : \cf5 // iPhone 15 Pro\cf2 \
              \cf3 return\cf2  \cf4 SensorInfo\cf2 (\
                  sensorHeight: \cf5 5.6\cf2 ,\
                  focalLengths: [\cf5 0.5\cf2 : \cf5 13\cf2 , \cf5 1.0\cf2 : \cf5 24\cf2 , \cf5 2.0\cf2 : \cf5 48\cf2 , \cf5 3.0\cf2 : \cf5 77\cf2 , \cf5 5.0\cf2 : \cf5 120\cf2 ]\
              )\
          \cf3 case\cf2  \cf6 "iPhone15,2"\cf2 : \cf5 // iPhone 14 Pro\cf2 \
              \cf3 return\cf2  \cf4 SensorInfo\cf2 (\
                  sensorHeight: \cf5 5.6\cf2 ,\
                  focalLengths: [\cf5 0.5\cf2 : \cf5 13\cf2 , \cf5 1.0\cf2 : \cf5 24\cf2 , \cf5 2.0\cf2 : \cf5 48\cf2 , \cf5 3.0\cf2 : \cf5 77\cf2 ]\
              )\
          \cf5 // ... \uc0\u44592 \u53440  \u47784 \u45944 \cf2 \
          \cf3 default\cf2 :\
              \cf3 return\cf2  \cf3 nil\cf2  \cf5 // \uc0\u44592 \u48376 \u44050  \u49324 \u50857 \cf2 \
          \}\
      \}\
  \}\
\
  
\f0\b 12.2 Depth Map \uc0\u49828 \u52992 \u51068  \u51221 \u44508 \u54868 
\f1\b0 \
\
  
\f0\b \uc0\u47928 \u51228 
\f1\b0 : Depth Anything\uc0\u51008  \u49345 \u45824 \u51201  \u44618 \u51060 \u47564  \u51228 \u44277  (\u51208 \u45824  \u44144 \u47532  \u50500 \u45784 )\
\
  
\f0\b \uc0\u54644 \u44208 \u52293 
\f1\b0 :\
  \cf5 /// \uc0\u44618 \u51060  \u47605  \u51221 \u44508 \u54868  (\u50612 \u44648  \u45320 \u48708  \u44592 \u51456 )\cf2 \
  \cf3 func\cf9  normalizeDepthMap(\cf2 \
  \cf9     depthMap: \cf4 CVPixelBuffer\cf9 ,\cf2 \
  \cf9     shoulderWidthPixels: \cf4 Float\cf9 ,\cf2 \
  \cf9     realShoulderWidth: \cf4 Float\cf9  = \cf5 0.4\cf9   \cf5 // 40cm\cf2 \
  \cf9 )\cf2  -> \cf4 Float\cf2  \{\
      \cf5 // 1. \uc0\u50612 \u44648  \u50689 \u50669 \u51032  \u54217 \u44512  \u44618 \u51060  \u52628 \u52636 \cf2 \
      \cf3 let\cf2  shoulderDepth = extractRegionDepth(depthMap, region: shoulderRegion)\
\
      \cf5 // 2. \uc0\u47932 \u47532 \u51201  \u44144 \u47532 \u47196  \u48320 \u54872 \cf2 \
      \cf5 // depth_ratio = pixel_width / sensor_width\cf2 \
      \cf5 // real_distance = real_shoulder_width / depth_ratio * depth_factor\cf2 \
      \cf3 let\cf2  scaleFactor = realShoulderWidth / shoulderWidthPixels\
      \cf3 let\cf2  estimatedDistance = scaleFactor * shoulderDepth * calibrationConstant\
\
      \cf3 return\cf2  estimatedDistance\
  \}\
\
  
